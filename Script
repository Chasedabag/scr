Mouse.Button2Down:Connect(function()
    M2Down = true

    while M2Down == true and Aimbot == true do
        task.wait()

        local Target = GetTarget()

        if Target then
            Camera.CFrame = Camera.CFrame:Lerp(CFrame.new(Camera.CFrame.Position,Target.Character.Head.Position),LockSpeed /15)
        end
    end
end)

Mouse.Button2Up:Connect(function()
    M2Down = false
end)

old = hookmetamethod(game,"__namecall",function(self,...)
    if SilentAim == true then
        if (getnamecallmethod() == "Raycast") then
            local CallingScript = getcallingscript()
            
            if CallingScript.Name ~= "GunScript_Local" then
                local target = GetTarget()
                local args = {...}
    
                if target then
                    args[2] = target.Character.Head.Position - args[1]
    
                    return old(self,unpack(args))
                end
            end
        end
    end

