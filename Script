oldindex = hookmetamethod(game,"__index",function(key,value)
    if tostring(key) == "HumanoidRootPart" and value == "Anchored" then
        warn("Game tried banning u, ez bypass")
        return false
    end

    return oldindex(key,value)
end)

local camera = game.Workspace.CurrentCamera
local Camera = game.Workspace.CurrentCamera
local UserInputService = game:GetService("UserInputService")
local UIP = game:GetService("UserInputService")

local Player = game.Players.LocalPlayer
local Mouse = Player:GetMouse()
_G.FovRadius = 250
_G.FovVisible = false
_G.FovColor = Color3.new(255,255,255)
_G.Thickness = 1

local Circle = Drawing.new("Circle")
Circle.Radius = _G.FovRadius
Circle.Visible = _G.FovVisible
Circle.NumSides = 128
Circle.Thickness = _G.Thickness
Circle.Color = _G.FovColor
Circle.Position = Vector2.new(UserInputService:GetMouseLocation().X,UserInputService:GetMouseLocation().Y)

game:GetService("RunService").RenderStepped:Connect(function()
    Circle.Radius = _G.FovRadius
    Circle.Visible = _G.FovVisible
    Circle.NumSides = 64
    Circle.Thickness = _G.Thickness
    Circle.Color = _G.FovColor
    Circle.Position = Vector2.new(UserInputService:GetMouseLocation().X,UserInputService:GetMouseLocation().Y)
end)

local function GetTarget()
    local Target = nil
    local LowestDistance = 100000000000000
    local Players = game.GetService(game,"Players")

    for i,v in pairs(Players.GetChildren(Players)) do
        if v.Name ~= Players.LocalPlayer.Name then
            if v.Character ~= nil then
                if v.Character.FindFirstChild(v.Character,"HumanoidRootPart") then
                    if v.Character.FindFirstChild(v.Character,"Humanoid") then
                        if v.Character.Humanoid.Health ~= 0 then
                            local ScreenPosition,onscreen = camera.WorldToViewportPoint(camera,v.Character.HumanoidRootPart.Position)
                            local CheckDistance = (Vector2.new(UserInputService.GetMouseLocation(UserInputService).X,UserInputService.GetMouseLocation(UserInputService).Y) - Vector2.new(ScreenPosition.X,ScreenPosition.Y)).Magnitude

                            if CheckDistance < _G.FovRadius and onscreen == true then
                                if CheckDistance < LowestDistance then
                                    LowestDistance = CheckDistance
                                    Target = v
                                end
                            end
                        end
                    end
                end
            end
        end
    end
    return Target
end

local FarmBoxes = false

local function SafeTween(cframe,tptype)
	local Dist = (Player.Character:GetPivot().Position - cframe.Position).Magnitude
	local TweenService = game:GetService("TweenService")

	local Tween = TweenService:Create(
		Player.Character.HumanoidRootPart,
		TweenInfo.new(Dist /25,Enum.EasingStyle.Linear),
		{CFrame = cframe}
	)
	local Tweening = true

	task.spawn(function()
		while Tweening == true do
			task.wait()

			if (Player.Character) and Player.Character:FindFirstChild("HumanoidRootPart") then
				Player.Character.Humanoid:ChangeState("Running")
				Player.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)

                if tptype == "boxes" then
                    if FarmBoxes == false then
                        Tween:Cancel()
                        Tweening = false
                    end
                end
			else
				Tween:Cancel()
				Tweening = false
			end
		end
	end)

	Tween:Play()
	Tween.Completed:Wait()
	Tweening = false
end

local function GetEquippedWeapon()
    if (Player.Character) and Player.Character:FindFirstChild("HumanoidRootPart") then
        for i,v in pairs(Player.Character:GetChildren()) do
            if (v:FindFirstChildWhichIsA("Tool")) and v:FindFirstChild("Gun") then
                print("Weapon Found!")
                return v
            end
        end
    end
end

