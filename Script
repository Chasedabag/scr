end)

FarmingSector.element('Toggle', 'Enable Box Farm', false, function(v)
    FarmBoxes = v.Toggle

    while FarmBoxes == true do
        task.wait()
    
        SafeTween(CFrame.new(Vector3.new(-551.304443, -4.64145708, -89.5524979)),"boxes")
        if FarmBoxes == false then
            break
        end
    
        local args = {
            [1] = "Box Job"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("FetchGUI"):FireServer(unpack(args))    
    
        Player.Character.HumanoidRootPart.Anchored = true
    
        wait(0.3)
        fireproximityprompt(workspace.PlaceHere:FindFirstChildWhichIsA("ProximityPrompt",true))
    
        local Crate = Player.Backpack:WaitForChild("Crate")
    
        wait(0.3)
    
        Crate.Parent = Player.Character
        Player.Character.HumanoidRootPart.Anchored = false
    
        SafeTween(CFrame.new(Vector3.new(-400, -4, -72)),"boxes")
        if FarmBoxes == false then
            break
        end
    
        --wait(1)
        Player.Character.HumanoidRootPart.Anchored = true
        fireproximityprompt(workspace.cratetruck2:FindFirstChildWhichIsA("ProximityPrompt",true))
        wait(0.3)
    
        local args = {
            [1] = "Box Job"
        }
        
        game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("FetchGUI"):FireServer(unpack(args)) 
        Player.Character.HumanoidRootPart.Anchored = false
    end

    Player.Character:PivotTo(Player.Character:GetPivot() + Vector3.new(0,7,0))
end)

MovementSector.element('Toggle', 'Enable Flight', false, function(v)
    Flight = v.Toggle

    while Flight == true do
        task.wait()
    
        if (Player.Character) and Player.Character:FindFirstChild("HumanoidRootPart") and FarmBoxes == false then
            local LookAt = Camera.CFrame.Position + Camera.CFrame.LookVector * 1000
            Player.Character.Humanoid:ChangeState("FreeFalling")
    
            if UIP:IsKeyDown(Enum.KeyCode.Space) then
                Player.Character:PivotTo(CFrame.new(Player.Character:GetPivot().Position + Vector3.new(0,0.3,0),Vector3.new(LookAt.X,Player.Character.HumanoidRootPart.Position.Y,LookAt.Z)))
            end 

            if UIP:IsKeyDown(Enum.KeyCode.LeftShift) then
                Player.Character:PivotTo(CFrame.new(Player.Character:GetPivot().Position - Vector3.new(0,0.3,0),Vector3.new(LookAt.X,Player.Character.HumanoidRootPart.Position.Y,LookAt.Z)))
            end 
            
            Player.Character:PivotTo(CFrame.new(Player.Character:GetPivot().Position + Player.Character.Humanoid.MoveDirection /25 * 5,Vector3.new(LookAt.X,Player.Character.HumanoidRootPart.Position.Y,LookAt.Z)))
            Player.Character.HumanoidRootPart.Velocity = Vector3.new(0,0,0)
        end
    end
end)

MovementSector.element('Toggle', 'Enable Speed', false, function(v)
    CFrameSpeed = v.Toggle

    while CFrameSpeed == true do
        task.wait()
        
        if (Player.Character) and Player.Character:FindFirstChild("Humanoid") and FarmBoxes == false then
            Player.Character:PivotTo(Player.Character:GetPivot() + Player.Character.Humanoid.MoveDirection /20 * CFrameSpeedAmount)
        end
    end
end)

