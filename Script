local SelectedSound = "rbxassetid://142376088"

FunSector.element('Dropdown', 'Sounds', {options = {"Raining Tacos","Metal Pipe Falling","Bypassed Audio Song","Nuke Siren","EarRaper","Sad Nigga Hours","German Song"}}, function(v)
    if v.Dropdown == "Raining Tacos" then
        SelectedSound = "rbxassetid://142376088"
    end

    if v.Dropdown == "Metal Pipe Falling" then
        SelectedSound = "rbxassetid://6011094380"
    end

    if v.Dropdown == "Bypassed Audio Song" then
        SelectedSound = "rbxassetid://6783714255"
    end

    if v.Dropdown == "Nuke Siren" then
        SelectedSound = "rbxassetid://6525690145"
    end

    if v.Dropdown == "EarRaper" then
        SelectedSound = "rbxassetid://3355609532"
    end

    if v.Dropdown == "Sad Nigga Hours" then
        SelectedSound = "rbxassetid://6806140478"
    end

    if v.Dropdown == "German Song" then
        SelectedSound = "rbxassetid://6950593634"
    end
end)

FunSector.element('Button', 'Play Sound At Me', nil, function()
    local args = {
        [1] = "PlaySound",
        [2] = {
            ["PlaybackSpeed"] = 1,
            ["SoundId"] = SelectedSound,
            ["SoundCFrame"] = Player.Character.HumanoidRootPart,
            ["Volume"] = 100,
            ["RollOffMode"] = Enum.RollOffMode.Inverse,
            ["RollOffMaxDistance"] = 1000000,
            ["TimePosition"] = 0,
            ["RollOffMinDistance"] = 10
        }
    }
    
    game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):WaitForChild("ClientEffects"):FireServer(unpack(args))
end)

FunSector.element('Button', 'Play Sound At Everyone', nil, function()
    for i,v in pairs(game.Players:GetChildren()) do
